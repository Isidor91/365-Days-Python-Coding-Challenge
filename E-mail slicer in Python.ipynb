    "email = input(\"Enter your e-mail: \")\n",
    "\n",
    "email=email.strip()\n",
    "\n",
    "slicer_index=email.index(\"@\")\n",
    "\n",
    "username = email[:slicer_index]\n",
    "domain_name = email[slicer_index+1:]\n",
    "\n",
    "print(\"Your username is \",username,\" and your domain is \",domain_name)\n",
    "\n"
