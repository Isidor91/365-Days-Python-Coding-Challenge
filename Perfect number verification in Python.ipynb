   "source": [
    "def perfectNumber(number):\n",
    "    sum = 0\n",
    "    for x in range(1, number):\n",
    "        if number % x == 0:\n",
    "            sum += x\n",
    "    return sum == number\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    \n",
    "    n=int(input(\"Enter a number to check : \"))\n",
    "    print(perfectNumber(n))"
   ]
